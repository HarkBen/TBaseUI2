<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rango.tbaseui" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/rango"
        android:label="@string/app_name"
        android:name="rango.CustomAPL"
        android:roundIcon="@mipmap/rango"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="rango.ui.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

<activity android:name="rango.ui.function.SlidingMenuAct1"></activity>
<activity android:name="rango.ui.function.SlidingMenuAct2"></activity>
<activity android:name="rango.ui.function.BottomNavigationAct"></activity>
<activity android:name="rango.ui.function.SwipeBackAct"></activity>
    </application>

</manifest>